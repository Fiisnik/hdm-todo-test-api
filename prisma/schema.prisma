generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Task {
  id        Int      @id @default(autoincrement())
  name      String
  description String?
  tag       String
  priority  String  
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}
